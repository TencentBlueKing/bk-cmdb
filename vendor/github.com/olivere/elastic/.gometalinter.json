{
  "Vendor": false,
  "Deadline": "2m",
  "Sort": ["linter", "severity", "path", "line"],
  "DisableAll": true,
  "Enable": [
    "goimports",
    "golint",
    "gosimple",
    "vet"
  ],
  "EnableGC": true,
  "WarnUnmatchedDirective": true,
  "Cyclo": 24,
  "LineLength": 200,
  "Exclude": [
    "generator",
    "spec",
    "tmp",
    ".*_easyjson.go"
  ]
}

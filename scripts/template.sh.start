#!/bin/bash

set -e

# get local IP.
localIp=`python ip.py`

# make log-dir if not exists.
if [[ ! -d "./logs" ]];then
    mkdir ./logs
fi

# set execute 
chmod +x cmdb-name-placeholder
./cmdb-name-placeholder --addrport=${localIp}:cmdb-port-placeholder --logtostderr=false --log-dir=./logs --v=3 --regdiscv=rd_server_placeholer > ./logs/std.log 2>&1 &
